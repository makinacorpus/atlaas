<div class="detail__heading">
	<h1 class="detail__title icon-actions"><%= titre %></h1>
	<% if (sous_titre.length) { %> <h2 class="detail__subtitle"><%= sous_titre %></h2><% } %>
	<button class="detail__bt--close icon-cross">close</button>
</div>
<div class="detail__meta">
	<h3 class="detail__location icon-location"><%= lieux[0].nom %></h3>
	<div class="detail__date">Publié le <%= date %></div>
	<a href="#edit/<%= id %>" class="detail__bt detail__bt--edit icon-edit">Modifier</a>
	<a href="<%= "data:text/csv;charset=utf-8," + encodeURIComponent(this.csv) %>" download="data.csv" class="detail__bt detail__bt--download icon-download" title="Télécharger la fiche sous format CSV">Télécharger</a>
	<div class="social">
		<a href="<%= window.location.href %>" class="twitter-share-button" data-lang="fr">Tweet</a>
		<div class="fb-share-button" data-href="<%= window.location.href %>" data-width="130" data-type="button_count"></div>
	</div>
</div>
<div class="detail__action">
	<a href="<%= services[0].enjeu %>" class="detail__breadcrumb filter" data-type="enjeu" data-name="<%= services[0].enjeu %>"><%=services[0].enjeu%></a><a href="<%= services[0].usage %>" class="detail__breadcrumb filter" data-type="usage" data-name="<%=services[0].usage%>" >> <%=services[0].usage%></a><a href="<%= services[0].id_service %>" class="detail__breadcrumb filter" data-type="service" data-name="<%=services[0].service%>">> <%=services[0].service%></a>
	<div class="row">
		<div class="detail__content col-8">
			<% if (synthese.length) { %>
			<h3>Synthèse</h3>
			<p><%= synthese %></p>
			<% } %>

			<% if (actions.length) { %>
			<h3>Actions</h3>
			<p><%= actions %></p>
			<% } %>

			<% if (resultats.length) { %>
			<h3>Résultats</h3>
			<p><%= resultats %></p>
			<% } %>

			<% if (typeof videos !== 'undefined' && videos.length) { %>
			<h3>Vidéos</h3>
			<p><%= videos %></p>
			<% } %>

			<% if (typeof photos !== 'undefined' && photos.length) { %>
			<h3>Photos</h3>
			<p><%= photos %></p>
			<% } %>

		</div>
		<div class="detail__related col-4">
			<% if (personnes.length) { %>
			<h3 class="icon-acteurs">Les acteurs</h3>
			<ul>
				<% _.each(personnes, function(personne) { %>
				<li class="actor">
					<span class="actor__name"><%= personne.nom %></span><a href="#map?actor=<%= personne.id_personne %>" class="detail__bt detail__bt--location filter" title="Voir toutes les actions de cet acteur" data-type="actor" data-id="<%= personne.id_personne %>" data-name="<%= personne.nom %>">+ Toutes les actions</a><br/>
					<span class="actor__title"><%= personne.titre %></span><br/>
					<% if (personne.elu.length) { %> <span><%= personne.elu %></span><br/> <% } %>
					<a class="actor__email" href="mailto:<%= personne.courriel %>" target="_blank"><%= personne.courriel %></a><br/>
				</li>
				<% }); %>
			</ul>
			<% } %>
			<% if (liens.length) { %>
			<h3>Liens utiles</h3>
			<%= liens %>
			<% } %>

			<% if (prestataires.length) { %>
			<h3>Les prestataires</h3>
			<%= prestataires %>
			<% } %>
		</div>
	</div>
</div>

<div id="fb-root"></div>
